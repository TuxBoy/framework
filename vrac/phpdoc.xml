<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
	<title>SAF - Simple Application Framework - Internals</title>
	<parser>
		<default-package-name>phpDocumentor</default-package-name>
		<target>data/output</target>
	</parser>
	<transformer>
		<target>data/output</target>
	</transformer>
	<transformations>
		<template name="new-black" />
	</transformations>
	<files>
		<directory>.</directory>
		<ignore>tests/*</ignore>
		<ignore>vendor/*</ignore>
	</files>
	<plugins>
		<plugin path="Core">
			<option name="a">value</option>
		</plugin>
	</plugins>
</phpdocumentor>
